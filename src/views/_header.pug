header.header
  nav.nav.nav--tours
    a.nav__el(href='/') 
      i.fa-solid.fa-house
  .header__logo
    h1 EndOw
  nav.nav.nav--user
    if user
      a.nav__el.nav__el--logout Log out
      a.nav__el(href='/me')
        img.nav__user-img(src=`/img/usericon.png` alt=`Photo of user`)
        span= user.username.split(' ')[0]
    else
      a.nav__el(href='/login') Log in
      a.nav__el.nav__el--cta(href='/signup') Sign up